<% for(var i = 0;i<users.length;i++){ %>
<tr>
    <td><%=i+1 %></td>
    <td><%=users[i].name%></td>
    <td><%=users[i].surname%></td>
    <td><%=users[i].nickname%></td>
    <td><%=users[i].email%></td>
    <td><%=users[i].address%></td>
    <td><%=users[i].phone%></td>
    <td><%= "*".repeat(users[i].password.length) %></td>
    <td><%=users[i].money%></td>
    <td><%=users[i].wins%></td>
    <td>
        <a href="/update-user?id=<%=users[i]._id%>" class="btn border-shadow update">
            <span class=""><i class="fas fa-pencil-alt"></i></span>
        </a>
        <a href="" class="btn border-shadow delete" data-id=<%=users[i]._id%>>
            <span class=""><i class="fas fa-times"></i></span>
        </a>
    </td>
</tr>
<% } %>
